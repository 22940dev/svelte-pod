{
  "name": "svelte-pod",
  "version": "1.0.0",
  "author": "Matthew Rebehn <matthew@mattaz.com>",
  "description": "Rankcasts - website to rank your favorite podcasts",
  "scripts": {
    "build": "rollup -c",
    "dev:network": "HOST=0.0.0.0 PORT=5000 npm run dev",
